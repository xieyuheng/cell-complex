note {
  endofunctor of set_category
}

container_t : type
container_t = conj {
  fun_t : (type) -> type
  map : ((a) -> b fun_t (a)) -> fun_t (b)
}

list_container : container_t
list_container = container_c {
  fun_t = list_t
  map (fun list) = {
    list case {
      null_t => null_c
      cons_t => cons_c (fun (list.car) map (fun list.cdr))
    }
  }
}
