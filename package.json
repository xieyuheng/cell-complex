{
  "name": "cicada-lang",
  "version": "0.0.11",
  "author": "Xie Yuheng",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git@github.com:xieyuheng/cicada.git"
  },
  "main": "lib/cicada.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "ava 2> /dev/null",
    "web-build": "parcel build web/index.html -d web/dist",
    "web-watch": "parcel web/index.html -d web/dist",
    "web-up": "surge web/dist cicada.surge.sh",
    "web": "npm run web-build; npm run web-up",
    "api-local": "typedoc src --out docs/api --mode modules --readme none",
    "api-up": "surge docs/api api.cicada.surge.sh",
    "api": "npm run api-local; npm run api-up",
    "up": "git commit -m 'up'; npm run build; npm run api; npm version patch; git push; npm publish"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "ava": "^1.4.1",
    "parcel-bundler": "^1.12.3",
    "surge": "^0.20.3",
    "ts-node": "^8.0.3",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.4.3",
    "@types/lodash": "^4.14.123",
    "lodash": "^4.17.11"
  }
}
